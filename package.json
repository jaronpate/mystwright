{
    "name": "mystwright",
    "version": "0.0.1",
    "packageManager": "yarn@3.8.5+sha512.786c27300bc8fb134a1a0630db71cfeccc2b92272c031df17ff662876e89c2df488e6819dd7f2ffaf02e021b6827f75d3eb6a6df0a10218f72a589b735db6cc2",
    "private": true,
    "scripts": {
        "run-tui": "bun --cwd=apps/tui run --env-file=../../.env index.ts",
        "run-api": "bun --cwd=apps/api run --env-file=../../.env start",
        "run-api:watch": "bun --cwd=apps/api run --env-file=../../.env watch",
        "run-web": "bun --cwd=apps/web run --env-file=../../.env dev",
        "build-web": "bun --cwd=apps/web run --env-file=../../.env build"
    },
    "workspaces": [
        "libs/*",
        "apps/*"
    ]
}
